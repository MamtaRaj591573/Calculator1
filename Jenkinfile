pipeline
	{ 
	agent any
	stages{
		stage ('Compile Stage')
		{
		steps{
			withMaven(maven : 'Maven_3.3.9')
			{
				sh 'mvn clean compile'
			}
		      }
		}
		stage ('Testing stage')
		{
		steps{
			withMaven(maven : 'Maven_3.3.9')
			{
				sh 'mvn Testing'
			}
		      }
		}
		stage ('Deployment stage')
		{
		steps{
			withMaven(maven : 'Maven_3.3.9')
			{
				sh 'mvn Deploy'
			}
		      }
		}
	}
}
